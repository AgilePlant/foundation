{
    "name": "agileplant/foundation",
    "description": "This is my package foundation",
    "keywords": [
        "agileplant",
        "laravel",
        "foundation"
    ],
    "homepage": "https://github.com/agileplant/foundation",
    "license": "MIT",
    "authors": [
        {
            "name": "AgilePlant",
            "email": "agileplant@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Foundation\\": "src/",
            "Foundation\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Foundation\\Foundation\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Foundation\\FoundationServiceProvider"
            ],
            "aliases": {
                "Foundation": "Foundation\\Facades\\Foundation"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
